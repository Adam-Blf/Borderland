# ===========================================
# BlackOut Hub - Variables d'Environnement
# ===========================================
# Copie ce fichier en .env et remplis les valeurs

# -----------------------------------------
# Application
# -----------------------------------------
VITE_APP_NAME=BlackOut
VITE_APP_VERSION=0.0.1
VITE_APP_URL=https://blackout.beloucif.com

# -----------------------------------------
# API Backend (optionnel - pour futur usage)
# -----------------------------------------
# VITE_API_URL=https://api.blackout.beloucif.com
# VITE_API_KEY=

# -----------------------------------------
# Analytics (optionnel)
# -----------------------------------------
# VITE_GA_ID=G-XXXXXXXXXX
# VITE_PLAUSIBLE_DOMAIN=blackout.beloucif.com

# -----------------------------------------
# Feature Flags
# -----------------------------------------
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false

# -----------------------------------------
# Supabase (si tu ajoutes une DB plus tard)
# -----------------------------------------
# VITE_SUPABASE_URL=https://xxxxx.supabase.co
# VITE_SUPABASE_ANON_KEY=

